{
  "presets": ["@babel/preset-env", "@babel/preset-react"],

  "extends": "next/core-web-vitals",
  "env": {
    "jest": true
  },
  "rules": {
    "react/no-unescaped-entities": "off",
    "react/jsx-first-prop-new-line": [2, "multiline"],
    "react/jsx-max-props-per-line": [2, { "maximum": 1, "when": "multiline" }]
  }
}
